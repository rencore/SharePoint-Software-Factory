				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="FontFamily" Value="{StaticResource FontFamily-Sketch}"/>
		<Setter Property="FontSize" Value="{StaticResource SizeDouble-Sketch}"/>
